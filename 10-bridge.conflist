{
  "cniVersion": "1.0.0",
  "name": "bridge",
  "type": "bridge",
  "bridge": "bridge0",
  "disableCheck": true,
  "ipam": {
    "type": "host-local",
    "subnet": "172.16.0.0/24",
    "gateway": "172.16.0.1",
    "routes": [
      { "dst": "0.0.0.0/0" }
    ]
  },
  "dns": {
    "nameservers": ["172.16.0.1"],
    "domain": "containerd.local"
  }
}
